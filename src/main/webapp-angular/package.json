{
  "name": "workflow",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prettier:write": "prettier --write \"**/src/**/*.{js,json,css,scss,less,md,ts,html}\" \"!**/src/api-client/**\"",
    "prettier:check": "prettier --check \"**/src/**/*.{js,json,css,scss,less,md,ts,html}\" \"!**/src/api-client/**\"",
    "start": "ng serve dev-shell --host 0.0.0.0",
    "build": "ng build library -c production && ng build library-web-components -c production && ng build nf-wrapper",
    "build:local": "ng build library -c development && ng build library-web-components -c development && ng build nf-wrapper",
    "watch": "ng build --watch --configuration development",
    "publish:snapshot": "echo 'no publish for dev-shells'",
    "unpublish:snapshot": "echo 'no unpublish for dev-shells'",
    "test": "ng test",
    "lint": "eslint ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.3",
    "@angular/cdk": "17.3.3",
    "@angular/common": "17.3.3",
    "@angular/compiler": "17.3.3",
    "@angular/core": "17.3.3",
    "@angular/elements": "17.3.3",
    "@angular/forms": "17.3.3",
    "@angular/platform-browser": "17.3.3",
    "@angular/platform-browser-dynamic": "17.3.3",
    "@angular/router": "17.3.3",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@vanillabp/bc-dev-shell-angular": "0.0.7-SNAPSHOT",
    "@vanillabp/bc-shared": "0.0.7-SNAPSHOT",
    "chart.js": "4.4.8",
    "es-module-shims": "2.0.10",
    "moment": "^2.30.1",
    "ng2-pdfjs-viewer": "^18.0.0",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primeng": "17.12.0",
    "quill": "2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "relative-time-format": "^1.1.6",
    "tslib": "2.6.2",
    "uuid": "^11.0.4",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "17.1.8",
    "@angular-builders/custom-webpack": "17.0.1",
    "@angular-devkit/build-angular": "17.3.0",
    "@angular/cli": "17.3.3",
    "@angular/compiler-cli": "17.3.3",
    "@eslint/js": "^9.3.0",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@softarc/native-federation-node": "^2.0.10",
    "eslint": "8.57.1",
    "globals": "^15.3.0",
    "ng-packagr": "17.3.0",
    "prettier": "3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "5.4.4",
    "typescript-eslint": "7.10.0",
    "webpack-cli": "5.1.4",
    "xml2js": "0.4.23"
  }
}
